@model Domain.Parc_auto

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-title">
                Edition Parc
            </div>
            <div class="panel-body">



                <div class="panel panel-default">


                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                    @Html.HiddenFor(model => model.Id_parc)
                        <div class="form-horizontal">
                            <h4>Parc_auto</h4>
                            <hr />
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                            <div class="form-group">
                                <label class="control-label col-lg-4" style="font-style:oblique;">Nom</label>
                                <div class="col-lg-6">

                                    @Html.TextBoxFor(model => model.Libelle, new { @class = "form-control", required = "" })

                                </div>
                            </div>




                            <div class="form-group">
                                <label class="control-label col-lg-4" style="font-style:oblique;">Delegation </label>
                                <div class="col-lg-6">
                                    @Html.DropDownListFor(model => model.idDelegation, (SelectList)ViewData["delegations"], "--Choisir--", new { @class = "form-control", required = "" })
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-lg-4" style="font-style:oblique;">Batiment </label>
                                <div class="col-lg-6">
                                    @Html.DropDownListFor(model => model.idBatiment, (SelectList)ViewData["batiment"], "--Choisir--", new { @class = "form-control", required = "" })
                                </div>
                            </div>

                        </div>
                        <div style="text-align: center;">
                            <input type="submit" value="Valider" class="btn btn-default" />

                            <button type="reset" class="btn btn-white">Annuler</button>

                        </div>

                    }
                </div>



            </div>
        </div>
    </div>
</div>
