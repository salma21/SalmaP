@model IEnumerable<Domain.Parc_auto>

<div class="row">
    <!-- Start Panel -->
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-title">
                Liste des Parc Auto
            </div>
            <div class="panel-body table-responsive">
                <table id="example0" class="table display">
                    <thead>
                        <tr>
                            <th>Nom  </th>
                           
                            <th>Region </th>
                            <th>Delegation </th>
                            <th>Action</th>
                        </tr>
                    </thead>

                    <tfoot>
                        <tr>
                            <th>Nom  </th>
                         
                            <th>Region </th>
                            <th>Delegation </th>

                            <th>Action</th>
                        </tr>
                    </tfoot>

                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>

                                <td class="center">@item.Libelle </td>
                                <td class="center">@item.Batiment.Delegation.Gouvernorat.Region.libelle</td>
                                <td class="center">@item.Batiment.Delegation.libelle</td>
                               
                                
                                <td>

                                    <a href="@Url.Action("EditParcAuto", new { id = item.Id_parc })">&nbsp;<i class="fa fa-edit" title="Modifer"></i></a>
                                </td>

                            </tr>


                        }
                    </tbody>
                </table>
                <br /><p>
                    @Html.ActionLink("Ajouter un nouveaux Parc", "CreateParcAuto")
                </p>

            </div>
        </div>
    </div>


</div>





<script>
    $(document).tooltip();
</script>