

<div class="inner">
    <!-- Start Row -->
    <div class="row">
        <!-- Start Panel -->
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-title">
                    Joindre Un Fichier
                </div>
                <div class="panel-body table-responsive">
                    <div id="div-1" class="accordion-body collapse in body">

                        @using (Html.BeginForm("ImportTxt", "ImportExport", FormMethod.Post, new { enctype = "multipart/form-data" }))
                        {
                            <div class="text-danger">@Html.Raw(ViewBag.Error)</div>
                            <div class="form-horizontal">
                                <table class="display" cellspacing="0" width="100%">
                                    <tbody>
                                        <tr>
                                            <td>
                                                <input type="file" id="fileToUpload" name="file" />
                                            </td>
                                            <td align="right" colspan="2">
                                                <input type="submit" class="btn btn-default" value="Charger le fichier" />
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        }


                    </div>
                </div>
            </div>
        </div>


    </div>
</div>