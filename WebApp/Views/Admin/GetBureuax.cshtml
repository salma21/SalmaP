@model IEnumerable<Domain.Bureau>


<!-- Start Row -->
<div class="row">
    <!-- Start Panel -->
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-title">
                Liste des Inventaires
            </div>
            <div class="panel-body table-responsive">
                <table id="example0" class="table display">
                    <thead>
                        <tr>
                            <th>Code Bureau</th>
                            <th>Code à barre</th>
                            <th>Description</th>
                            <th>Direction</th>
                            <th>Etage</th>
                            <th>Batiment</th>
                        
                            <th>Action</th>
                        </tr>
                    </thead>

                    <tfoot>
                        <tr>
                            <th>Code Bureau</th>
                            <th>Code à barre</th>
                            <th>Description</th>
                            <th>Direction</th>
                            <th>Etage</th>
                            <th>Batiment</th>

                            <th>Action</th>
                        </tr>
                    </tfoot>

                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td class="center">@item.Id_bureau</td>
                                <td class="center">@item.Code_a_barre</td>
                                <td class="center">@item.Description</td>
                                <td class="center">@item.Direction.Libelle</td>
                                <td class="center">@item.Etage.description</td>
                                <td class="center">@item.Etage.Batiment.description</td>
                             
                                <td>
                                   <a href="@Url.Action("EditBureaux", new { id = item.id })">&nbsp;<i class="fa fa-edit" title="Modifer"></i></a>
                                    </td>

                            </tr>


                        }
                    </tbody>
                </table>
                <br /><p>
                    @Html.ActionLink("Ajouter un nouveau Bureau", "CreateBureaux")
                </p>

            </div>



            </div>



        </div>
    </div>

<script>
    $(document).tooltip();
</script>