@model IEnumerable<Domain.Societe_assurance>

<!-- Start Row -->
<div class="row">
    <!-- Start Panel -->
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-title">
                Liste des Societes d'Assurance
            </div>
            <div class="panel-body table-responsive">
                <table id="example0" class="table display">
                    <thead>
                        <tr>

                            <th>Region</th>

                            <th>Delegation</th>
                            <th>Societe</th>
                            <th>Rue</th>
                            <th>Tel</th>
                            <th>Fax</th>
                            <th>Email</th>

                            <th>Action</th>
                        </tr>
                    </thead>

                    <tfoot>
                        <tr>
                            <th>Region</th>

                            <th>Delegation</th>
                            <th>Societe</th>
                            <th>Rue</th>
                            <th>Tel</th>
                            <th>Fax</th>
                            <th>Email</th>
                           
                            <th>Action</th>
                        </tr>
                    </tfoot>

                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>

                                <td class="center">@item.Delegation.Gouvernorat.Region.libelle</td>
                                <td class="center">@item.Delegation.libelle</td>
                                <td class="center">@item.Libelle </td>
                                <td class="center">@item.Rue</td>
                                <td class="center">@item.Tel</td>
                                <td class="center">@item.Fax</td>
                                <td class="center">@item.Email</td>
                                

                                <td>
                                    <a href="@Url.Action("Edit", new { id = item.Id_societe_assurance })">&nbsp;<i class="fa fa-edit" title="Modifer"></i></a>
                                    <a href="@Url.Action("Delete", new { id = item.Id_societe_assurance })">&nbsp;&nbsp;<i class="fa fa-trash" title="Effacer"></i></a>
                                </td>

                            </tr>


                        }
                    </tbody>
                </table>
                <br /><p>
                    @Html.ActionLink("Ajouter une Societe", "CreateSociete_assurance")
                </p>

            </div>



        </div>



    </div>
</div>

<script>
    $(document).tooltip();
</script>



