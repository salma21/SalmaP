@model IEnumerable<Domain.Contrat_maintenance>

    <!-- Start Row -->
    <div class="row">
        <!-- Start Panel -->
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-title">
                    Liste des Contrat de Maintenance
                </div>
                <div class="panel-body table-responsive">
                    <table id="example0" class="table display">
                        <thead>
                            <tr>

                                <th>Region</th>

                                <th>Delegation</th>
                                <th>Societe</th>
                                <th>Maintenance</th>
                               
                                <th>Date Deb</th>
                                <th>Date Fin</th>
                                <th>Cout</th>


                                <th>Action</th>
                            </tr>
                        </thead>

                        <tfoot>
                            <tr>
                                <th>Region</th>

                                <th>Delegation</th>
                                <th>Societe</th>
                                <th>Maintenance</th>

                                <th>Date Deb</th>
                                <th>Date Fin</th>
                                <th>Cout</th>


                                <th>Action</th>
                            </tr>
                        </tfoot>

                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>

                                    <td class="center">@item.Societe_maintenance.Delegation.Gouvernorat.Region.libelle</td>
                                    <td class="center">@item.Societe_maintenance.Delegation.libelle</td>
                                    <td class="center">@item.Societe_maintenance.Libelle  </td>
                                    <td class="center">@item.Type_maintenance</td>

                                    <td class="center">@item.Date_debut</td>
                                    <td class="center">@item.Date_fin</td>
                                    <td class="center">@item.Cout</td>


                                    <td>
                                        <a href="@Url.Action("Edit", new { id = item.Id_contrat_assurance })">&nbsp;<i class="fa fa-edit" title="Modifer"></i></a>
                                        <a href="@Url.Action("Delete", new { id = item.Id_contrat_assurance })">&nbsp;&nbsp;<i class="fa fa-trash" title="Effacer"></i></a>
                                    </td>

                                </tr>


                            }
                        </tbody>
                    </table>
                    <br /><p>
                        @Html.ActionLink("Ajouter un nouveau Contrat de Maintenance", "CreateContrat_Maintenance")
                    </p>

                </div>



            </div>



        </div>
    </div>

    <script>
    $(document).tooltip();
    </script>



