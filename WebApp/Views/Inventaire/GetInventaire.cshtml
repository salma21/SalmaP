@model IEnumerable<Domain.Inventaire>

<div class="row">
    <!-- Start Panel -->
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-title">
                Liste des Batiments
            </div>
            <div class="panel-body table-responsive">
                <table id="example0" class="table display">
                    <thead>
                        <tr>
                            <th>Nom </th>
                            <th>Type</th>
                            <th>Date</th>
                            <th>Nombre Des Biens Affectes </th>

                           

                            <th>Action</th>
                        </tr>
                    </thead>

                    <tfoot>
                        <tr>
                            <th>Nom </th>
                            <th>Type</th>
                            <th>Date</th>
                            <th>Nombre Des Biens Affectes </th>

                            <th>Action</th>
                        </tr>
                    </tfoot>

                    <tbody>
                        @foreach (var item in Model)
                        {
                            
                            <tr>

                                <td class="center">@item.Nom_</td>
                                <td class="center">@item.Type</td>
                                <td class="center">@item.Date</td>
                                <td class="center">25</td>
                               
                                <td>
                                    <a href="@Url.Action("AddBien", new { id = item.Id_inventaire })">&nbsp;<i class="fa fa-plus-square" title="Ajouter Des Biens"></i></a>
                                    <a href="@Url.Action("Edit", new { id = item.Id_inventaire })">&nbsp;<i class="fa fa-edit" title="Modifer"></i></a>
                                   
                                </td>

                            </tr>


                        }
                    </tbody>
                </table>
                <br /><p>
                    @Html.ActionLink("Ajouter un Nouveau Inventaire", "CreateInventaire")
                </p>

            </div>
        </div>
    </div>


</div>





<script>
    $(document).tooltip();
</script>